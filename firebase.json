{
  "functions": [
    {
      "source": "dist/analog/server",
      "runtime": "nodejs18",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ]
    }
  ],
  "hosting": {
    "site": "kaliahayes-com-analog",
    "public": "dist/analog/public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,
    "rewrites": [
      {
        "function": "server",
        "source": "**"
      }
    ]
  }
}
